<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gxkj.projects.myshopx.dao.UserT">

	<resultMap id="BaseResultMap" type="com.gxkj.projects.myshopx.entitys.UserT" >
		     <id column="id" property="id"   />
		      <result column="user_name" property="userName" jdbcType="VARCHAR" />
		      <result column="password" property="password" jdbcType="VARCHAR" />
		     <result column="age" property="age" jdbcType="BIGINT" />
	</resultMap>
	
	<select id="getPageUserT"   resultMap="BaseResultMap"  >
		SELECT id,user_name,password,age FROM user_t a
	</select>
	<insert id="addUser"  parameterType="com.gxkj.projects.myshopx.entitys.UserT"  useGeneratedKeys="false"  keyProperty="id">
		insert into user_t
		(id,user_name,password,headUrl) values (
		#{id},#{userName},#{password},#{headUrl}
		)
	</insert>

</mapper>